pipeline {
agent any
tools{ jdk 'jdk' }
environment { JAVA_HOME = 'C:\\Program Files\\Java\\jdk-11.0.16' }
stages {
stage ('Compile Stage') {
steps {
withMaven(maven : 'maven') {
bat 'mvn clean compile'
}}
}
stage ('Testing Stage') {
steps {
withMaven(maven : 'maven') {
bat 'mvn test'
} }}
stage ('Install Stage') {
steps {
withMaven(maven : 'maven') {
bat 'mvn install'
} } } }}

